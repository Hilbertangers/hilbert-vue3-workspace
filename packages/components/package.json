{
  "name": "hilbert-vue3-ui",
  "version": "0.5.0",
  "description": "一个现代化的 Vue3 组件库",
  "type": "module",
  "keywords": [
    "vue",
    "vue3",
    "component",
    "ui",
    "hilbert"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hilbertangers/hilbert-vue3-workspace"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./lib/index.js",
      "types": "./es/index.d.ts"
    },
    "./style": "./es/index.css",
    "./*": {
      "import": "./es/*/index.js",
      "require": "./lib/*/index.js"
    },
    "./*/style": "./es/*/index.css"
  },
  "sideEffects": [
    "*.less",
    "src/**/*.less"
  ],
  "files": [
    "lib",
    "es",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c && npm run build:types",
    "build:types": "vue-tsc -p tsconfig.build.json",
    "dev": "rollup -c -w",
    "release": "npm publish"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/element-resize-detector": "^1.1.6",
    "@types/node": "^24.10.0",
    "@vue/compiler-sfc": "^3.4.0",
    "fast-glob": "^3.3.3",
    "less": "^4.4.2",
    "rollup": "^4.9.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-vue": "^6.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3",
    "vue-tsc": "^2.1.8"
  },
  "dependencies": {
    "element-resize-detector": "^1.2.4",
    "es-toolkit": "^1.41.0",
    "fuse.js": "^7.1.0"
  }
}
